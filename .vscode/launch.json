  "version": "0.2.0",
  "configurations": [
    {
      "name": "dump rally",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/rally-to-anything.py",
      "args": ["dump-rally", "-v"],
      "console": "integratedTerminal"
    },
    {
      "name": "dump rally - clear cache",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/rally-to-anything.py",
      "args": ["dump-rally", "-v", "-c"],
      "console": "integratedTerminal"
    },
    {
      "name": "migrate jira",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/rally-to-anything.py",
      "args": ["migrate-jira", "-v"],
      "console": "integratedTerminal"
    }
  ]
}
